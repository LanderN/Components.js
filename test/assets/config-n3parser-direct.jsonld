{
  "@context": {
    "@vocab": "http://linkedsoftwaredependencies.org/vocabularies/components#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "ex": "http://example.org/",
    "n3": "http://example.org/n3#",
    "npm": "http://linkedsoftwaredependencies.org/ontologies/npm#"
  },
  "@graph": [
    {
      "@id": "ex:myParser",
      "@type": [ "n3:Parser", "ComponentConstructable" ],
      "npm:requireName": "n3",
      "lsdc:componentPath": "Parser",
      "arguments": [
        {
          "format": "application/trig",
          "lexer": { "@id": "n3:lexer" }
        }
      ]
    },
    {
      "@id": "ex:myLexer",
      "@type": [ "n3:Lexer", "ComponentConstructable" ],
      "npm:requireName": "n3",
      "lsdc:componentPath": "Lexer",
      "arguments": {
        "@list": [
          {
            "@type": "Object",
            "hasField": [
              {
                "rdfs:label": "comments",
                "rdf:value": true
              }
            ]
          }
        ]
      }
    }
  ]
}
